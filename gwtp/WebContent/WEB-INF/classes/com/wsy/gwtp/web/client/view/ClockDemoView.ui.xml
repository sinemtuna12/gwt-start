<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui' ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys='com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator'
	ui:generateLocales='default'>

	<ui:style src="../resources/css/clockdemo.css" />
		
	<g:HTMLPanel>
		<div class="{style.wrapper}">
			<div class="{style.clock} {style.move}">
				<div class="{style.points}">
					<div class="{style.point} {style.p1}"></div>
					<div class="{style.point} {style.p2}"></div>

					<div class="{style.point} {style.p4}"></div>
					<div class="{style.point} {style.p5}"></div>

					<div class="{style.point} {style.p7}"></div>
					<div class="{style.point} {style.p8}"></div>

					<div class="{style.point} {style.p10}"></div>
					<div class="{style.point} {style.p11}"></div>

				</div>
				<div class="{style.numbers}">
					<div class="{style.number} {style.n12}">12</div>
					<div class="{style.number} {style.n3}">3</div>
					<div class="{style.number} {style.n6}">6</div>
					<div class="{style.number} {style.n9}">9</div>
				</div>
				<div class="{style.tune}">
					<div class="{style.hour} {style.hand}"></div>
				</div>
				<div class="{style.tune}">
					<div class="{style.minute} {style.hand}"></div>
				</div>
				<div class="{style.tune}">
					<div class="{style.secondWrapper}">
						<div class="{style.second} {style.hand}"></div>
					</div>
				</div>
				<div class="{style.center} {style.point}"></div>
				<div class="{style.glass}"></div>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
